@model CRUD1.Models.Employee

<h2>Create Employee</h2>

<form method="post">

    <label>First Name</label>
    <input class="form-control" asp-for="FirstName" />

    <label>Last Name</label>
    <input class="form-control" asp-for="LastName" />

    <label>Age</label>
    <input class="form-control" asp-for="Age" />

    <label>Date Of Hire</label>
    <input class="form-control" asp-for="DateOfHire" type="date" />

    <label>Email Address</label>
    <input class="form-control" asp-for="Email" />

    <label>Position</label>
    <input class="form-control" asp-for="Position" />

    <label>Salary</label>
    <input class="form-control" asp-for="Salary" />

    <button class="btn btn-success mt-3" type="submit">Submit</button>
    <a class="btn btn-danger mt-3" href="/Employee/Index">Back</a>
</form>